{
  "name": "@myst-theme/jupyter",
  "version": "0.3.3",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx\" -c ./.eslintrc.js",
    "lint:format": "prettier --check \"src/**/*.{ts,tsx,md}\"",
    "dev": "npm-run-all --parallel \"build:* -- --watch\"",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "build": "npm-run-all -l clean -p build:cjs build:esm build:types",
    "test": "vitest run"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "@myst-theme/providers": "^0.3.3",
    "ansi-to-react": "^6.1.6",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "myst-common": "^1.0.1",
    "myst-config": "^1.0.0",
    "myst-frontmatter": "^1.0.0",
    "myst-spec": "^0.0.4",
    "myst-to-react": "^0.3.2",
    "nanoid": "^4.0.2",
    "nbtx": "^0.2.3",
    "react-syntax-highlighter": "^15.5.0",
    "swr": "^2.1.5",
    "thebe-core": "^0.2.5",
    "thebe-lite": "^0.2.5",
    "thebe-react": "^0.2.5",
    "unist-util-select": "^4.0.3"
  },
  "peerDependencies": {
    "@types/react": "^16.8 || ^17.0 || ^18.0",
    "@types/react-dom": "^16.8 || ^17.0 || ^18.0",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  }
}
