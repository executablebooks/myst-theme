{
  "name": "myst-to-react",
  "version": "0.2.9",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint \"src/**/*.ts*\" \"src/**/*.tsx\" -c ./.eslintrc.js",
    "lint:format": "prettier --check \"src/**/*.{ts,tsx,md}\"",
    "dev": "npm-run-all --parallel \"build:* -- --watch\"",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "build:esm": "tsc --module es2020 --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "build": "npm-run-all -l clean -p build:cjs build:esm build:types"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "@myst-theme/providers": "^0.2.9",
    "@radix-ui/react-hover-card": "^1.0.5",
    "buffer": "^6.0.3",
    "classnames": "^2.3.2",
    "myst-common": "^0.0.17",
    "myst-config": "^0.0.16",
    "myst-spec": "^0.0.4",
    "nanoid": "^4.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "swr": "^1.3.0",
    "unist-util-select": "^4.0.3"
  },
  "peerDependencies": {
    "@types/react": "^16.8 || ^17.0 || ^18.0",
    "@types/react-dom": "^16.8 || ^17.0 || ^18.0",
    "react": "^16.8 || ^17.0 || ^18.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/react-syntax-highlighter": "^15.5.6",
    "eslint": "^8.21.0",
    "eslint-config-curvenote": "latest",
    "npm-run-all": "^4.1.5",
    "tsconfig": "latest",
    "typescript": "latest"
  },
  "overrides": {
    "ansi-to-react": {
      "react": "^16.8 || ^17.0 || ^18.0",
      "react-dom": "^16.8 || ^17.0 || ^18.0"
    }
  }
}
